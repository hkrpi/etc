auto lo

iface lo inet loopback

auto eth0
iface eth0 inet dhcp

# eth1 should be the 3g/3g modem
allow-hotplug eth1
auto eth1
iface eth1 inet dhcp

iface wlan0 inet static
  address 10.68.9.1
  netmask 255.255.255.0

